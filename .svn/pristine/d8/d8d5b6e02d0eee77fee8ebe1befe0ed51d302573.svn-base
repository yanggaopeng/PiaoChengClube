/*导航栏*/
.Header{
    width:956px;
    height: 152px;
    margin-top: 10px;
}
/*主要内容*/
.content{
    width:954px;
    height:600px;
    border:1px solid #CDCDCD;
    margin-top: 10px;
}
.content h3{
    line-height:25px;
    text-align: center;
    background-color: #EEEEEE;
    font-size: 14px;
}
.content h4{
    width:118px;
    line-height: 18px;
    font-size: 12px;
    margin-top: 10px;
    font-weight: bold;
}
.form{
    margin-top: 8px;
    width:419px;
    height:325px;
}
.form-type{
    height:45px;
    margin-bottom: 6px;
}
.form-type span,.form-title span,.form-map span{
    text-align: right;
    display:block;
    float:left;
    width:66px;
    height:45px;
    margin-right: 4px;
    letter-spacing: 8px;
}
.form-type ul{
    float:left;
    width:313px;
    height:45px;
}
.form-type ul li{
    float:left;
    line-height: 18px;
    margin-right: 3px;
    margin-bottom: 6px;
}
.form-type ul li img{
    width:15px;
    height:16px;
}
.form-title{
    height:28px;
    margin-bottom: 3px;
}
.form-title input{
    float: left;
    width:247px;
    height:18px;
    border:1px dashed #333333;
}
.form-title p{
    float: left;
    width:90px;
    line-height: 20px;
    font-size: 12px;
    margin-top: 4px;
}
.form-map{
    height:17px;
    margin-bottom: 6px;
}
.form-map p{
    float:left;
    width:70px;
    line-height: 15px;
    font-size: 14px;
    margin-right: 10px;
}
.form-text{
    float: left;
    width:409px;
    height:169px;
    border:1px dashed #333333;
    margin-bottom: 8px;
}
.form-text ul{
    height:26px;
    border-bottom:2px solid #A9A9A9;
    background-image: url("../Img/images/bgMenubar.gif");
}
.form-text ul li{
    float:left;
    margin: 4px 3px;
}
.form-yzm{
    float:left;
    height:25px;
}
.form-yzm p{
    float: left;
    text-align: center;
    font-size:14px;
    width:59px;
    line-height:25px;
    letter-spacing: 2px;
}
.form-yzm input{
    float:left;
    width:58px;
    line-height: 20px;
    border:1px dashed #333333;
}
.form-yzm img{
    float:left;
    width:65px;
    height:22px;
    margin-left: 4px;
    margin-right: 4px;
}
.form-yzm button{
    float:left;
    width:80px;
    line-height: 20px;
    border:1px dashed #333333;
    background-color: white;
    font-size: 12px;
}
/*底部*/
.footer{
    width:956px;
    height:156px;
    margin-top: 10px;
}