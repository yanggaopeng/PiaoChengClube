/*头部*/
.top{
    width:956px;
    height:152px;
    margin-top: 10px;
}
/*主体内容*/
.content{
    width:954px;
    height:600px;
    margin-top: 10px;
    border:1px solid #CDCDCD;
}
.content h3{
    text-align: center;
    background-color:#EEEEEE;
    line-height: 25px;
}
.thieu{
    width:116px;
    height:15px;
    font-size: 12px;
    font-weight: bold;
    margin-top: 10px;
}
.form{
    width:398px;
    height:430px;
    margin-top: 10px;
    margin-left: 273px;
}
.form div{
    height:30px;
}
.form div p{
    float:left;
    width:64px;
    font-size:12px;
    text-align: right;
    margin-top: 10px;
    letter-spacing: 2px;
}
.form div input{
    margin-top: 5px;
    margin-left: 3px;
    line-height: 20px;
    border:1px dashed #333333;
}
.form div span{
    font-size: 14px;
    color:#333333;
}
.form div button{
    width:57px;
    line-height:18px;
    border:1px dashed #333333;
    background-color: white;
    font-size: 12px;
    margin-left: 118px;
    margin-top: 10px;

}
/*底部*/
.footer{
    width:956px;
    height:80px;
}